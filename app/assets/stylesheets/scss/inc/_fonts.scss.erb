// Nunito Regular
@font-face {
  font-family:'Nunito';
  src: url('<%=  asset_url 'fonts/nunito/nunito-regular.eot' %>');
  src: url('<%=  asset_url 'fonts/nunito/nunito-regular?#iefix' %>') format('embedded-opentype'),
  url('<%=  asset_url 'fonts/nunito/nunito-regular.woff2' %>') format('woff2'),
  url('<%=  asset_url 'fonts/nunito/nunito-regular.woff' %>') format('woff'),
  url('<%=  asset_url 'fonts/nunito/nunito-regular.svg#Nunito Regular' %>') format('svg');
  font-weight: 400;
  font-style: normal;
  font-stretch: normal;
}


@font-face {
  font-family:'Nunito';
  src: url('<%=  asset_url 'fonts/nunito/nunito-semibold.eot' %>');
  src: url('<%=  asset_url 'fonts/nunito/nunito-semibold.eot?#iefix' %>') format('embedded-opentype'),
  url('<%=  asset_url 'fonts/nunito/nunito-semibold.woff2' %>') format('woff2'),
  url('<%=  asset_url 'fonts/nunito/nunito-semibold.woff' %>') format('woff'),
  url('<%=  asset_url 'fonts/nunito/nunito-semibold.svg#Nunito SemiBold' %>') format('svg');
  font-weight: 600;
  font-style: normal;
  font-stretch: normal;
  unicode-range: U+0020-2212;
}


