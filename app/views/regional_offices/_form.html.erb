
<%= simple_form_for([@three_pl, @regional_office]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :office_description %> 
    <%= f.input :street %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :postal %>
    <%= f.input :phone %>
    <%= f.input :fax %>
    <%= f.input :email %>
    <%= f.input :notes %>
    <%= f.input :three_pl_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
