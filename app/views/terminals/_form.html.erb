
<%= simple_form_for [@app_setting, @terminal] do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :street %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :zip %>
    <%= f.input :telephone %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
