<p id="notice"><%= notice %></p>

<h1>Terminals</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Telephone</th>
      <th>App setting</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @terminals.each do |terminal| %>
      <tr>
        <td><%= terminal.name %></td>
        <td><%= terminal.street %></td>
        <td><%= terminal.city %></td>
        <td><%= terminal.state %></td>
        <td><%= terminal.zip %></td>
        <td><%= terminal.telephone %></td>
        <td><%= terminal.app_setting_id %></td>
        <td><%= link_to 'Show', app_setting_terminal_path(@app_setting, terminal) %></td>
        <td><%= link_to 'Edit', app_setting_terminal_path(@app_setting, terminal) %></td>
        <td><%= link_to 'Destroy', app_setting_terminal_path(@app_setting, terminal), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Terminal', new_app_setting_terminal_path %>
